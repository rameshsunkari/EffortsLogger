 <application>
 	<message-bundle>resources.application</message-bundle>
 	<locale-config>
 		<default-locale>en</default-locale>
 	</locale-config>
  <resource-bundle>
   <base-name>com.efforts.messages.LabelMessages</base-name>
   <var>lblMsg</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>com.efforts.messages.Message</base-name>
   <var>Msg</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>com.efforts.messages.ErrorMessages</base-name>
   <var>errMsg</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>com.efforts.messages.MailMessage</base-name>
   <var>mailMsg</var>
  </resource-bundle>
 </application>
</faces-config>
